create table if not exists cliente(idCliente int primary key auto_increment, nomeCliente varchar(45),
tipoCliente varchar(45), cpfCliente varchar(45), rgCliente varchar(45), dataDeNascCliente date, enderecoCliente varchar(45),
cepCliente varchar(45), telefoneCliente varchar(45), dddCliente varchar(45), emailCliente varchar(120));
